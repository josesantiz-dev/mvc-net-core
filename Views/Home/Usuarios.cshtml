@using HolaMVC.Models   

@{
    int i = 2;
    List<int> Numeros = new List<int>(){
        1,2,3,4,5,6,7,8,9
    };
    int a = 5*1;
    int Dia = 2;
}
<ul>
    @foreach (int Numero in Numeros)
    {
       if(Numero %2 == 0){
           if(Numero < 5){
               <li>@Numero</li>
           }
       }
    }
</ul>
<ul>
    @for (int j = 0; j < Numeros.Count-1; j++)
    {
        <li>@Numeros[i]</li>
    }
</ul>
@switch (Dia)
{
    case 1:
        <span>Lunes</span>
        break;
    case 2:
        <span>Martes</span>
        break;
    case 3:
        <span>Miercoles</span>
        break;
    
}
@{
    List<Usuario> usuarios = (List<Usuario>)ViewData["Usuarios"];
    int Total = usuarios.Count;
}

<table class="table table-striped table-bordered">
    <header>
        <tr>
            <th>Nombre</th>
            <th>Apellidos</th>
            <th>Correo</th>
            <th>Edad</th>
        </tr>
    </header>
    <tbody>
        @for (int k = 0; k <= Total -1; k++)
        {
            <tr>
            <td>@usuarios[k].Nombre</td>
            <td>@usuarios[k].Apellidos</td>
            <td>@usuarios[k].Correo</td>
            <td>@usuarios[k].Edad</td>
            </tr>
        }
    </tbody>
</table>